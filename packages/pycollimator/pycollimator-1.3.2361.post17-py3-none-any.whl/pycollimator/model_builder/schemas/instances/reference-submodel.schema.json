{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://collimator.ai/models/reference-submodel.schema.json",

  "title": "ReferenceSubmodel",
  "description": "Reference submodel",
  "type": "object",

  "properties": {
    "$schema_version": {
      "type": "string",
      "description": "Schema version",
      "default": "0.1",
      "enum": ["0.1"]
    },
    "uuid": { "$ref": "uuid.schema.json" },
    "name": {
      "type": "string",
      "description": "Name of this model"
    },
    "diagram": { "$ref": "model-diagram.schema.json" },
    "parameters": { "$ref": "parameter-definitions.schema.json" },
    "submodels": { "$ref": "submodels-section.schema.json" }
  },
  "required": [
    "$schema_version",
    "uuid",
    "name",
    "diagram",
    "submodels",
    "parameters"
  ],
  "$comment": "submodels should be required. Versions are not supported here. This type is used in export/import of models from clipboard and in the chat for conversion to python code where versions are not used."
}
